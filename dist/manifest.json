{
  "name": "Remove Tweet Views",
  "description": "Removes tweet view counts from twitter",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-380.png"
  },
  "icons": {
    "380": "icon-380.png"
  },
  "host_permissions": ["*://twitter.com/*"],
  "permissions": [],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["*://twitter.com/*"]
    }
  ]
}
